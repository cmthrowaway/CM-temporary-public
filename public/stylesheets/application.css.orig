/******************* =General Styles (All Pages) *******************/

body {

}

h1 {
	font-size:3em;
}

h2 {
	font-size:2em;
	margin:0.5em 0;
}

h3 {
	margin:0.5em 0;
}

h4 {
	
}

h5 {
	
}

h6 {
	
}

h1, h2, h3, h4, h5, h6 {
	color:#117fbc;
}

a {
	color:#74a035;
	font-weight:bold;
	text-decoration:none;
}

em {
	font-style:italic;
}

strong {
	font-weight:bold;
}

p {
	color:#808080;
}

li.last {
	border-right:none !important;
}

#register #flash_notice,
#login #message {
	margin-bottom:1.7em;
}

#register #flash_notice p,
#login #message p {
	color:green;
	font-weight:bold;
}

#register #flash_notice p.big,
#login #message p.big {
	font-size:2em;
	margin-bottom:0.2em;
}

/******************* =Header (All Pages) *******************/

#header {
	margin-bottom:1em;
	position:relative;
}

#header p#tagline {
	margin-top:2.7em;
	margin-left:11em;
	position:relative;
	top:2.7em;
}

/* =Main Nav */

#header #nav {
	background: url('/images/backgrounds/back-header-nav.png') no-repeat;
	clear:both;
	height:30px;
}

#header a#logo img {
	float:left;
	margin-bottom:2em;
}

#header #nav ul {
	margin-left:1em;
	padding-top:0.3em;
}

#header #nav ul li {
	border-right:1px solid white;
	float:left;
}

#header ul li a {
	display:block;
	font-family:"Helvetica Neue",Arial,Helvetica,Geneva,sans-serif;
	font-size:1.1em;
	text-decoration:none;
	text-transform:capitalize;
	padding:0.2em 1em 0 1em;
}

#header #nav ul li a {
	color:white;
}

/* =Top Nav */

#header ul#top-nav {
	position:absolute;
	right:0em;
}

#header ul#top-nav li {
	border-right:2px solid #117FBC;
	float:left;

}

#header ul#top-nav li a {
	padding-top:0.2em;
}

/* =Search */

#header #search {
	background: url('/images/backgrounds/back-header-nav.png') no-repeat;
	background-color:blue;
	float:right;
	margin-right:3.4em;
}

#header #search input#search_q {
	color:#bcb7b7;
	font-family:"Myriad Set","Lucida Grande",Helvetica,Arial,sans-serif;
	font-size:1em;
	width:22em;
}

#header #search input#search_submit {
	border:1px solid #838383;
	font-family:"Myriad Set","Lucida Grande",Helvetica,Arial,sans-serif;
	font-size:1.1em;
	padding:0 0.2em;
}


/******************* =Main Content (All Pages) *******************/

#main-content {
	float:left;
	margin-bottom:5em;	
}

/******************* =Footer (All Pages) *******************/

#footer {
	clear:both;
	margin-top:5em;
}

#footer ul#links {
	float:left;
	margin-left:17em;
	margin-bottom:1em;
}

#footer ul#links li {
	float:left;
	border-right:1px solid #117FBC;
	padding:0 20px;
}

#footer ul#links li.last {
	border:none;
}

#footer #copyright {
	clear:both;
}

#footer #copyright p {
	font-size:0.9em;
	left:104px;
	position:relative;
	top:-25px;
	width:73em;
}

