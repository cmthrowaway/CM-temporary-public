<div id="add-company">
  <h2>Add Company</h2>
  <%= error_messages_for :company %>
  <% form_for(:company, :url => collection_url) do |f| %>
    <%= render :partial => "form", :locals => { :f => f } %>
    <p>
      <%= submit_tag "Add", :id => "submit" %>
      <a id="back" href="/admin" title="Back to Administer Companies">Back</a>
    </p>
  <% end %>
</div><!-- end add-company -->
