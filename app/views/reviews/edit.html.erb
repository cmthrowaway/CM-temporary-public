<h1>Editing Review</h1>

<%= error_messages_for :review %>


<% form_for([@company, @review], :html => {:method => :put}) do |f| %>
  <% raise "f.object is nil" if f.object.nil? %>
	<%= render :partial => "form", :locals => { :f => f } %>
  <p>
    <%=submit_tag "Update"%>
  </p>
<% end %>

<br/>

<%= link_to 'Back', reviews_path %>
