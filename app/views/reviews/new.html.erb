<div id="review_new"class="form_box">
  <div class="content_header">
    <h2>New Review</h2>
  </div>
  <div class="content_body">

    <%= error_messages_for :review %>

    <% form_for(:review_presenter, @review, :url => collection_url) do |f| %>
      <%= render :partial => "form", :locals => { :f => f, :review => @review } %>
      <div class="submit">
        <%= image_submit_tag "button_submit.gif" %>
      </div>
      <br>
    <% end %>
  </div>
</div>